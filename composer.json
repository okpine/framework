{
    "name": "demo/framework",
    "type": "project",
    "description": "The Demo Framework.",
    "keywords": [
        "framework",
        "Demo"
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Demo\\App\\": "src/web",
            "Demo\\Framework\\": "src/framework"
        },
        "files": [
            "src/framework/Foundation/functions.php"
        ]
    },
    "require": {
        "php-di/php-di": "^6.2",
        "nyholm/psr7": "^1.3",
        "slim/psr7": "^1.2",
        "psr/http-server-middleware": "^1.0",
        "psr/http-server-handler": "^1.0",
        "monolog/monolog": "^2.1",
        "symfony/var-dumper": "^5.1",
        "nyholm/psr7-server": "^1.0",
        "nikic/fast-route": "^1.3",
        "vlucas/phpdotenv": "^5.2"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
